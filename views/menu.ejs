    </div>

    <script type="module" src="https://unpkg.com/ionicons@7.1.0/dist/ionicons/ionicons.esm.js"></script> <!-- Skript pro ikony (module) -->
    <script nomodule src="https://unpkg.com/ionicons@7.1.0/dist/ionicons/ionicons.js"></script> <!-- Skript pro ikony (nomodule) -->

</body> <!-- Konec těla stránky -->
</html> <!-- Konec HTML kódu -->
